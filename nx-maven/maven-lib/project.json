{
  "name": "maven-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "./nx-maven/maven-lib/src",
  "targets": {
    "install": {
      "executor": "@jnxplus/nx-maven:run-task",
      "outputs": ["{projectRoot}/target", "{options.outputDirLocalRepo}"],
      "options": {
        "task": "install -DskipTests=true"
      }
    },
    "build": {
      "executor": "@jnxplus/nx-maven:run-task",
      "outputs": ["{projectRoot}/target", "{options.outputDirLocalRepo}"],
      "options": {
        "task": "install -DskipTests=true"
      }
    },
    "test": {
      "executor": "@jnxplus/nx-maven:run-task",
      "options": {
        "task": "test"
      }
    }
  },
  "tags": []
}
